{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "managementGroupId": {
      "value": "YOUR_MANAGEMENT_GROUP_ID"
    },
    "location": {
      "value": "westeurope"
    },
    "environment": {
      "value": "sandbox"
    },
    "organizationPrefix": {
      "value": "alz"
    },
    "hubVnetAddressSpace": {
      "value": "10.0.0.0/16"
    },
    "spokeVnetAddressSpace": {
      "value": "10.1.0.0/16"
    },
    "enableAzureFirewall": {
      "value": false
    },
    "enableBastion": {
      "value": true
    },
    "applicationWorkloads": {
      "value": {
        "enableWebApp": true,
        "enableContainerApps": true,
        "enablePostgreSQL": true,
        "enableStorage": true,
        "enableAppGateway": true
      }
    }
  }
}
