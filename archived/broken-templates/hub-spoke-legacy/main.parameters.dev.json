{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "westeurope"
    },
    "environment": {
      "value": "dev"
    },
    "organizationPrefix": {
      "value": "alz"
    },
    "hubVnetAddressPrefix": {
      "value": "10.0.0.0/16"
    },
    "spokeVnetAddressPrefix": {
      "value": "10.1.0.0/16"
    },
    "enableFirewall": {
      "value": true
    },
    "enableBastion": {
      "value": true
    },
    "enableDnsResolver": {
      "value": true
    },
    "enableAppGateway": {
      "value": true
    },
    "enableWebApps": {
      "value": true
    },
    "enableContainerApps": {
      "value": true
    },
    "enablePostgreSQL": {
      "value": true
    },
    "enableStorage": {
      "value": true
    },
    "firewallSkuTier": {
      "value": "Standard"
    },
    "dbAdministratorLogin": {
      "value": "alzadmin"
    },
    "dbAdministratorPassword": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/{subscription-id}/resourceGroups/{keyvault-rg}/providers/Microsoft.KeyVault/vaults/{keyvault-name}"
        },
        "secretName": "db-admin-password"
      }
    }
  }
}
